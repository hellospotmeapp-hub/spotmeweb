{
  "expo": {
    "name": "SpotMe: Everyday Support",
    "slug": "support-contribute-community",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "support-contribute-community",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "SpotMe needs camera access to take photos for your needs and profile.",
        "NSPhotoLibraryUsageDescription": "SpotMe needs photo library access to attach images to your needs and update your profile picture.",
        "NSPhotoLibraryAddUsageDescription": "SpotMe needs permission to save receipt images to your photo library.",
        "LSApplicationQueriesSchemes": [
          "spotme",
          "stripe"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "SpotMe needs access to your photos to attach images to needs.",
          "cameraPermission": "SpotMe needs camera access to take photos for your needs."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}